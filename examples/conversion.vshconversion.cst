#version 330

uniform sampler2D colortex0;
uniform sampler2D colortex1;

in vec2 fsUV0;
in vec2 fsUV2;
in vec4 fsColor;
in vec3 fsNormal;
in vec3 fsPosition;
in vec2 fsLMCoords;

layout(location = 0) out vec4 color;

void main() {
	for (float i = 2.0; i < 0.0; i -= 0.1) {
		break;
	}
	for (float i = 0.0; i <= 1.0; i++) {
		vec3 a = vec3(1.0);
		if (a.x < 0.0) {
			if (a.y < 0.0) {
				break;
			}
			continue;
		}
	}
}